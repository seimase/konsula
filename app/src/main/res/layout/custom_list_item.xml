<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="55dip"
    android:orientation="horizontal"
    >

    <RelativeLayout
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <com.konsula.app.ui.custom.CustomtextView
            android:id="@+id/lblListItem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingRight="@dimen/space_60"
            android:layout_marginBottom="@dimen/space_10"
            android:paddingLeft="@dimen/space_18"
            android:layout_marginTop="@dimen/space_10"
            android:text="ASD"
            android:textColor="@color/black"
            android:textSize="14dp" />

        <com.konsula.app.ui.custom.CustomtextView
            android:id="@+id/lblListItemRight"
            android:layout_width="wrap_content"
            android:layout_toLeftOf="@+id/lblListItemRight2"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/space_8"
            android:layout_marginTop="@dimen/space_8"
            android:textColor="@color/black"
            android:textSize="14dp" />

        <com.konsula.app.ui.custom.CustomtextView
            android:id="@+id/lblListItemRight2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/space_14"
            android:paddingTop="1dp"
            android:textColor="@color/black"
            android:textSize="@dimen/space_8"
            android:layout_alignTop="@+id/lblListItem"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/space_14" />

    </RelativeLayout>

</RelativeLayout>